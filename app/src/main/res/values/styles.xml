<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <!--<item name="android:fontFamily">customfontstyle</item>-->
        <!--<item name="android:typeface">@font/julius_sans_one</item>-->
        <item name="android:fontFamily">@font/raleway</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowBackground">@color/transparent</item>

        <!--<item name="android:fontFamily">@font/raleway</item>-->

    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="AppTheme.NoActionBar.AppBarOverlay"
           parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="AppTheme.NoActionBar.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>


    <!--<item name="android:fontFamily">@font/lobster</item>-->
    <style name="customfontstyle" parent="@android:style/TextAppearance">
        <item name="android:fontFamily">@font/raleway</item>
    </style>
    <style name="navstyle" parent="TextAppearance.AppCompat.Medium">
        <item name="android:fontFamily">@font/raleway_medium</item>
    </style>
    <style name="toolbarstyle" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:fontFamily">@font/raleway_bold</item>
    </style>
    
    <!--<style name="borderedButton" parent="Base.TextAppearance.AppCompat.Widget.Button.Borderless.Colored" >-->
        <!--<item name="bo"-->
    <!--</style>-->


</resources>
